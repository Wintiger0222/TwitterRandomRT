{
  "name": "Twitter Random RT",
  "version": "0.0.0.1",
  "description": "Chooses Random User Who Retweeted Tweet",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Twitter Random RT",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
