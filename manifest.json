{
  "name": "Twitter Random RT",
  "version": "1.0.0",
  "description": "Choosing Random User Who Retweeted",
  "permissions": ["tabs"],
  "host_permissions": ["https://*.twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_title": "Twitter Random RT",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
